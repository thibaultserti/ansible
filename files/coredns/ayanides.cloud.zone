$ORIGIN ayanides.cloud.  ; designates the start of this zone file in the namespace
$TTL 1h               ; default expiration time of all resource records without their own TTL value
@                 IN  SOA     ns.ayanides.cloud. core.ayanides.cloud. (
                                  2023060719     ; Serial
                                  1d             ; Refresh
                                  2h             ; Retry
                                  4w             ; Expire
                                  1h)            ; Minimum TTL
@                 IN  A       192.168.0.40       ; Local IPv4 address for ayanides.cloud.
                  IN  NS      ns.ayanides.cloud. ; Name server for ayanides.cloud.
ns                IN  CNAME   @                  ; Alias for name server (points to ayanides.cloud.)

; Compute
box               IN  A       192.168.0.1
eros              IN  A       192.168.0.40
tycho             IN  A       192.168.0.41
pallas            IN  A       192.168.0.42
ceres             IN  A       192.168.0.44


; FQDN
semaphore.ayanides.cloud.         IN  A 192.168.0.40
